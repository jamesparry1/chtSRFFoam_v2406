        (0 -0.1975 0.1140)	//0.95
        (0 -0.2078 0.1200)	//1.00 (top shroud)
	// Adjacent probes
        (-0.01 -0.1247 0.0720)	//0.60 US
        (0.01 -0.1247 0.0720)	//0.60 DS
        (-0.01 -0.1767 0.1020)	//0.85 US
        (0.01 -0.1767 0.1020)	//0.85 DS
	//rb = 0.85 pressure probe
	//(-0.0199 -0.1767 0.1020)  //US
	//(0.0199 -0.1767 0.1020)   //DS

	//US Pressure Probes
        (-0.0199 -0.1143 0.0660)      //0.55
        (-0.0199 -0.1247 0.0720)      //0.60
        (-0.0199 -0.1351 0.0780)      //0.65
        (-0.0199 -0.1455 0.0840)      //0.70
        (-0.0199 -0.1559 0.0900)      //0.75
        (-0.0199 -0.1663 0.0960)      //0.80
        (-0.0199 -0.1767 0.1020)      //0.85
        (-0.0199 -0.1871 0.1080)      //0.90
        (-0.0199 -0.1975 0.1140)      //0.95

	//DS Pressure Probes
        (0.0199 -0.1143 0.0660)      //0.55
        (0.0199 -0.1247 0.0720)      //0.60
        (0.0199 -0.1351 0.0780)      //0.65
