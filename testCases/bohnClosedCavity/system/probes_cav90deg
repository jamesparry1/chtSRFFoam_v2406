        (0 0 -0.216)	//0.90
        (0 0 -0.228)	//0.95
        (0 0 -0.239)	//1.00 (top shroud)
	// Adjacent probes
        (-0.03 0 -0.144)//0.60 US
        (0.03 0 -0.144)	//0.60 DS
        (-0.03 0 -0.204)//0.85 US
        (0.03 0 -0.204)	//0.85 DS
	//rb = 0.85 pressure probe
	//(-0.0599 0 -0.204)  //US
	//(0.0599 0 -0.204)   //DS

//	//pressure probe array
