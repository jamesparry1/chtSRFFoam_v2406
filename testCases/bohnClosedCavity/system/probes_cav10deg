        (-0.01 0.1418 -0.0250)	//0.60 US
        (0.01 0.1418 -0.0250)	//0.60 DS
        (-0.01 0.2009 -0.0354)	//0.85 US
        (0.01 0.2009 -0.0354)	//0.85 DS
	//rb = 0.85 pressure probe
	//(-0.0199 0.2009 -0.0354)  //US
