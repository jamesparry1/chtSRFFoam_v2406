        (0 0.2271 -0.0199)	//0.95
        (0 0.2391 -0.0209)	//1.00 (top shroud)
	// Adjacent probes
        (-0.01 0.1435 -0.0126)	//0.60 US
        (0.01 0.1435 -0.0126)	//0.60 DS
        (-0.01 0.2032 -0.0178)	//0.85 US
        (0.01 0.2032 -0.0178)	//0.85 DS
	//rb = 0.85 pressure probe
	//(-0.0199 0.2032 -0.0178)  //US
	//(0.0199 0.2032 -0.0178)   //DS

	//US Pressure Probes
        (-0.0199 0.1315 -0.0115)      //0.55
        (-0.0199 0.1435 -0.0126)      //0.60
        (-0.0199 0.1554 -0.0136)      //0.65
        (-0.0199 0.1674 -0.0146)      //0.70
        (-0.0199 0.1793 -0.0157)      //0.75
        (-0.0199 0.1913 -0.0167)      //0.80
        (-0.0199 0.2032 -0.0178)      //0.85
        (-0.0199 0.2152 -0.0188)      //0.90
        (-0.0199 0.2271 -0.0199)      //0.95
	//DS Pressure Probes
        (0.0199 0.1315 -0.0115)      //0.55
        (0.0199 0.1435 -0.0126)      //0.60
        (0.0199 0.1554 -0.0136)      //0.65
        (0.0199 0.1674 -0.0146)      //0.70
